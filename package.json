    "typeface-martel": "0.0.54",
    "typeface-montserrat": "0.0.54",
    "typeface-open-sans": "0.0.54",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/node": "^10.12.9",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-helmet": "^5.0.7",
    "@types/react-share": "^2.1.1",
    "gh-pages": "^2.0.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-blvd": "^1.2.1",
