    "images": "node scripts/generate-images.js",
    "clean-start": "npm run clean && npm start",
    "production": "gatsby build && gatsby serve",
    "generate": "plop --plopfile ./generators/plopfile.js",
    "clean-modules": "rimraf node_modules && npm install"
